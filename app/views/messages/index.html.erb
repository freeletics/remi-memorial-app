<section class="headline text-center">
  <%= link_to "Leave your message", new_message_path, class: "btn btn-primary" %>
</section>

<p class="notice"><%= notice %></p>

<ul id="messages" class="message-list message-list-index">
  <%= render partial: "message", collection: @messages %>
</ul>

<%= paginate @messages, params: {anchor: "messages"} %>
